			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"First name is Janet\", function () {",
							"    const data = pm.response.json().data;",
							"    pm.expect(data.first_name).to.eql(\"Janet\");",
							"});",
							"",
							"pm.test(\"Email format is valid\", function () {",
							"    const email = pm.response.json().data.email;",
							"    pm.expect(email).to.match(/.+@.+\\..+/);",
							"});",
							"",
							"pm.test(\"Avatar URL exists\", function () {",
							"    const avatar = pm.response.json().data.avatar;",
							"    pm.expect(avatar).to.be.a(\"string\");",
							"    pm.expect(avatar.length).to.be.above(5);",
							"});",
							""
